/* layout */
.mrWrap {overflow: hidden; width:100%; background: #F2F2F2;}
.mrWrap .mrHeader {z-index: 9999; position: fixed; top: 0; width: 100%; height:50px; border-bottom:1px solid #ccc; background:#22272B;}
.mrWrap .mrHeader .headerArea {width:980px; margin:0 auto;}

.mrWrap .mrContainer {position: relative; width:980px; margin:0 auto; background: #fff;}
.mrWrap .mrContainer:after {content:""; display:block; clear:both;}
.mrWrap .mrFooter {text-align:center; padding: 10px 0; background: #d7d4d0;}
.mrWrap .mrFooter .footerArea {}

/* header */
.headerArea h1 {float:left; width:150px; height:50px;}
.headerArea .gnbWrap {float:left; line-height: 50px;}
.headerArea .gnbWrap li {float:left; padding:0 24px 0 25px; background:url(../../images/gnb_bar.png) no-repeat left center;}
.headerArea .gnbWrap li a {color:#FCFCD4; font-size: 12px; text-decoration: none;}
.headerArea .gnbWrap li a:hover {color: #ccc;}
.headerArea .gnbWrap li .on {color: #FFCC33; font-weight: bold;}

.headerArea .lnbWrap {position:relative; float:right; width: 333px; padding:15px 0 0;}

/* login 전 .lnbWrap */
.headerArea .lnbWrap.beforeLogin {position:relative; float:right; width: 333px; text-align: right; padding:15px 0 0;}

.headerArea .lnbWrap.beforeLogin .mChart {}
.headerArea .lnbWrap.beforeLogin .mChart a {color: #ccc}
.headerArea .lnbWrap.beforeLogin .mChart a:hover {color: #fff}
.headerArea .lnbWrap.beforeLogin .mChart span {border: 1px solid #ccc}

.headerArea .lnbWrap.beforeLogin .bulInfo {padding: 0 10px;}
.headerArea .lnbWrap.beforeLogin .bulMail {padding: 0 10px 0 0;}
.headerArea .lnbWrap.beforeLogin a.goLogin {display: inline-block; color: #F4E454; padding: 0 0 0 10px;}
.headerArea .lnbWrap.beforeLogin a.joinUs {display: inline-block; color: #fff; padding: 0 0 0 10px;}

/* login 후 .lnbWrap */
.headerArea .lnbWrap .mChart {}
.headerArea .lnbWrap .mChart a {color: #FFCC33}
.headerArea .lnbWrap .mChart a:hover {color: #F8EA8E}
.headerArea .lnbWrap .mChart span {border: 1px solid #FFCC33}

.headerArea .lnbWrap .bulInfo {padding: 0 10px;}
.headerArea .lnbWrap .bulMail {padding: 0 10px 0 0;}
.headerArea .lnbWrap .topaycWrap {}
.headerArea .lnbWrap .dropType1 li.div {border-bottom: 1px solid #ccc}

/* title */
.hType1 {clear: both; overflow: hidden; padding: 30px 0 20px;}
.hType1 h3 {float: left; color: #444; padding: 0 0 0 22px; font-size: 15px; background: url(../../images/bul_h3.gif) no-repeat 0 3px;}
.hType1 p {float: left; padding: 3px 0 0 15px; font-size: 12px;}

/* btn */
.btn {display:inline-block !important; margin:0; color: #fff; background: #666; text-decoration:none !important; border:0; font-size:12px; white-space:nowrap; overflow:visible;}
a.btn span {display:inline-block; border:1px solid #999; padding: 1px 10px 3px; vertical-align: middle;}
a.btn:hover {color: #fff; background: #999;}

/* contents */
.mrContainer .contents {position: relative; width:940px; min-height: 700px; margin:50px auto 0; padding:25px 20px 100px;}
.mrContainer .contents:after {content:""; display:block; clear:both;}
.contents .titleWrap {padding:0 0 10px; border-bottom:2px solid #ccc; height:35px}
.contents h2 {float:left; color: #444; padding: 10px 0 0; font-size:18px;}
.contents .conArea {padding: 30px 0 0;}

.accuntArea {}
.accuntArea h3 {color: #000; clear: both;color: #333; padding: 0 0 10px 20px;font-size: 14px;font-weight: bold;background: url(../../images/bul_h4.gif) no-repeat 0 4px;}
.accuntArea .searchpPriod {border: 1px solid #ccc; padding: 10px;}

.checkboxArea {padding: 10px 0 30px;}

.tblScroll {overflow-y: scroll; height: 500px;}


/* popup */
.layerPopupWrap {}
.layerPopupWrap .outer {padding: 20px; border: 2px solid #ddd;}






































