	//JSON이용한 리스트 받아오기.
	AutoAddLine.arrangement_list = function(){};
	AutoAddLine.prototype.arrangement_list = function(){
		
			var param = "?page=" + AutoAddLine.page + 
							"&listType=" + AutoAddLine.listType + 
							"&genreNo=" + AutoAddLine.genreNo + 
							"&orderBy=" + AutoAddLine.orderby + 
							"&searchWord=" + AutoAddLine.searchWord + 
							"&isSearch=" + AutoAddLine.isSearch;

			if(AutoAddLine.listType==1){
				var url="/music/compose/composeRemoteList" + param;  // 작곡 리스트
			}else if(AutoAddLine.listType==2 || AutoAddLine.listType==3){
				var url="/music/arrange/arrangeRemoteList" + param;  // 진행중 및 완료 편곡			
			}else if(AutoAddLine.listType==4){
				var url="/music/vocal/vocalRemoteList" + param;  //음원 리스트(보컬 리스트)			
			}

//			alert(url);
			
		   $.getJSON(url, function(data) {

	              $.each(data, function(key, state){
	            	  
	            	  if(key=='totalPage'){
	            		  if(state<=0){
	            			  AutoAddLine.addRow_End_Mark();	            			  
	            			  AutoAddLine.scrollLock=true;
	            		  }
	            	  }
	            	  
	            	  if(key=='objects'){  //objects만 추출
	            		  $.each(state, function(key2, state2){
	            			  
								if(AutoAddLine.listType==1){
									AutoAddLine.addRowType1(state2);   //작곡리스트 가져오기
								}else if(AutoAddLine.listType==2 || AutoAddLine.listType==3){
									AutoAddLine.addRowType2(state2);   //편곡리스트 및  진행중인 편곡 가져오기.		
								}else if(AutoAddLine.listType==4){
									AutoAddLine.addRowType3(state2);   //곡 리스트 가져오기					
								}
	            		  })	            		  
	            	  }
	              });
	          });

	         AutoAddLine.scroll_again();
	};

	// 스크롤 이벤트 중복발생 방지용 (릴레이).	
	AutoAddLine.scroll_again = function(){};	
